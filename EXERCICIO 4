    <script>
        let ano_nas = Number(prompt("Digite seu ano de nascimento"));
        let idade = 2025 - ano_nas
        console.log(idade)
            if(idade <=12){
            console.log("Você é uma Criança");
        }else if(idade<18){
            console.log("Você é um Adolecente");
        }else if(idade<60){
            console.log("Você é um adulto");
        } else if(idade<110){
            console.log("Você é um idoso");
        }

    </script>

